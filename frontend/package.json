{
  "name": "nexinvo-frontend",
  "version": "1.0.0",
  "description": "NexInvo - Invoice Management System Frontend",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node scripts/update-version.js patch && react-scripts build",
    "build:no-version": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "version:patch": "node scripts/update-version.js patch",
    "version:minor": "node scripts/update-version.js minor",
    "version:major": "node scripts/update-version.js major",
    "build:prod": "node scripts/update-version.js patch && set REACT_APP_API_URL=https://www.nexinvo.chinmaytechnosoft.com/api && react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
